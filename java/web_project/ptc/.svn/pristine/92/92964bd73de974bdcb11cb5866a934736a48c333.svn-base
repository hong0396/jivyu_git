   <script type="text/javascript"> 
 $.cxSelect.defaults.url = '../../json/cityData.min.json'; 
 $.cxSelect.defaults.nodata = 'none';  
  // selects 为数组形式，请注意顺序 
$('#element_id').cxSelect({ 
required:true,
  url: '../../json/cityData.min.json',   // 提示：如果服务器不支持 .json 类型文件，请将文件改为 .js 文件 
  selects: ['province', 'city'], 
  nodata: 'none' 
}); 
  </script>
  <script type="text/javascript"> 
    function queryPage(){    
    myChart.showLoading({
					text : '数据获取中',
					effect: 'whirling'
				}),				
   $.ajax({
            //提交数据的类型 POST GET
            type:"POST",
            //提交的网址
            url:"../../echartsServlet",
            //提交的数据            
             data:{id:$("#city").val(),OUT_SITE_CODE:$("#OUT_SITE_CODE").val(),date:$("#date").val()},
            //返回数据的格式
            datatype: "json",//"xml", "html", "script", "json", "jsonp", "text".
            //在请求之前调用的函数
            // beforeSend:
            //成功返回之后调用的函数             
            success:function(data){ 
            	myChart.hideLoading(); 
       
              option.series[0].markLine.data=data;    

			  myChart = BMapExt.initECharts(container);
			  myChart.clear();
			  
              BMapExt.setOption(option); 
             
              var json=eval(data);
              var msg="";
              var total=0;
              var i;
  for( i=0; i<data.length; i++)  
  {  //jQuery("<div class="row"> <span class="serial-number col">2</span><a href="javascript:;" class="col city-name">天津</a><i class="col i i11"></i><a href="javascript:;" class="col-3 city-name">北京</a><span class="percent"><span data="perMil">23.5</span>％</span> </div>").appendTo(jQuery("#"+list));
  	  if(i<15){
  	  	msg=msg+"<div class='row'> <span class='serial-number col'>"+(i+1)+"</span>";
  	  	  	  
  	    msg=msg+"<a style='color: #0088cc' href='javascript:;' class='col city-name'>"+json[i][0].name+"</a>";
  	      	   
  	    msg=msg+"<i class='col i i11'></i><a style='color: #0088cc' href='javascript:;'  class='col-3 city-name'>"+json[i][1].name+"</a>";  
  	    	    
  	    msg=msg+"<span class='percent'><span data='perMil'>"+json[i][1].value+"</span>";  	  
  	   
  	    msg=msg+"</span></div>";
  	    
  	    total=total+parseInt(json[i][1].value);
  	    
  	    continue;
  	    }
  	    total=total+parseInt(json[i][1].value);
 
    // alert(json[i][0].name+" "+json[i][1].name+" " + json[i][1].value);  
  } $("#list").empty();       
   $("#list").append(msg);
   //alert(total);
   var no=i<15?(i+1):16;
   $("<div class='row'> <span class='serial-number col'>"+(no)+"</span><a style='color: #0088cc' href='javascript:;' class='col city-name'>"+'汇总'+"</a><span class='percent'><span data='perMil'>"+total+"</span></span> </div>").appendTo($("#list"));  
            },
            //调用执行后调用的函数
            complete: function(XMLHttpRequest, textStatus){ 
              
            },
            //调用出错执行的函数
            error: function(){
                //请求出错处理
            }         
         });

  }  
  </script>
  
  <link href="../../laydate-master/skins/yalan/laydate.css"  rel="stylesheet" type="text/css"  /> 
  <script type="text/javascript" src="../../laydate-master/laydate.dev.js"></script>
 
<script>
laydate({
    elem: '#date', //目标元素。由于laydate.js封装了一个轻量级的选择器引擎，因此elem还允许你传入class、tag但必须按照这种方式 '#id .class'
    format: 'YYYYMMDD',
    festival: true, //显示节日
   // event: 'focus' //响应事件。如果没有传入event，则按照默认的click
});
</script>
 
